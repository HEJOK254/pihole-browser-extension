		"sass-loader": "^10.0.5",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.0.9",
		"ts-node": "^9.0.0",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.0.5",
		"vue-class-component": "^7.2.6",
