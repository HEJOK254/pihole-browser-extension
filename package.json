		"bootstrap-vue": "^2.15.0",
		"jquery": "^3.5.1",
		"vue": "^2.6.11"
	},
	"devDependencies": {
		"@types/bootstrap": "^4.5.0",
		"@types/chrome": "0.0.104",
		"@types/firefox-webext-browser": "^70.0.1",
		"@types/jquery": "^3.5.0",
		"@types/node": "^14.0.14",
		"@types/webpack": "^4.41.18",
		"@types/webpack-dev-server": "^3.11.0",
		"copy-webpack-plugin": "^5.1.1",
		"css-loader": "^3.6.0",
		"html-webpack-plugin": "^4.2.1",
		"node-sass": "^4.14.1",
